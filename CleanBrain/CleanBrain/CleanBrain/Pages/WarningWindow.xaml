<Window x:Class="CleanBrain.Pages.WarningWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CleanBrain.Pages"
        Background="Transparent"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        WindowStyle="None"
        Title="WarningWindow" Height="296" Width="500">
    <Border Background="#284751">
        <Grid>
            <Border VerticalAlignment="Bottom" Width="200" Margin="0 0 0 10">
                <Button Grid.Row="8" Width="200" Opacity="1"  Style="{StaticResource Button_Prof}" Content="Ok" Height="70" VerticalAlignment="Center" HorizontalAlignment="Center" Click="Оk_Click"/>
            </Border>
            <TextBlock Name="Messange"  Style="{StaticResource Textgrey}" Foreground="White" Opacity="1" TextWrapping="Wrap"  VerticalAlignment="Center" FontSize="38" Grid.Row="0" Height="110" Margin="30,67,30,89.4"/>
        </Grid>
    </Border>
   
</Window>
